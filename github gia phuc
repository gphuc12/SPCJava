package spc.edu;
import java.util.Scanner;
class Sienvien{
    private String ten;
    private double diem;
    
    
    public void setTen(String ten ){
        this.ten=ten;
    }
    public void setDiem(double diem){
        this.diem=diem;
    }
    public double getDiem (){
        return diem;
        
    }
 *
 * @author Administrator
 */
public class Baii22_SapxepObject {
    public static void main(String[] args) {
        String ten;
        double diem;
        Scanner sc = new Scanner (System.in);
        Systen. out. println("NHAP n: ");
        int n = Integer.parseInt(sc.nextLine());
        Sienvien sv [] = new Sinhvien[n];
        for(int i = 0;1 < n; i++){
          sv[i] = new Sinhvien();
          ten = sc.nextLine();
          sv[i].setTen(ten);
          System.out.println("Nhap ten");
          diem =sc.nextDouble();
          
    }
    }
}
